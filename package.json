{
  "name": "@timada/semantic-release-cargo",
  "version": "1.0.5",
  "description": "semantic-release plugin to publish a cargo package.",
  "files": [
    "lib/*.js"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/timada-org/semantic-release-cargo.git"
  },
  "author": "snapiz",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "ava",
    "build": "tsc"
  },
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@types/node": "^18.16.0",
    "@types/semantic-release": "^20.0.1",
    "ava": "^5.2.0",
    "semantic-release": "^21.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "lib/"
      },
      "compile": "tsc"
    },
    "files": [
      "src/test/*.ts"
    ],
    "workerThreads": false,
    "timeout": "2m"
  },
  "dependencies": {
    "@ltd/j-toml": "^1.38.0"
  }
}
